<!DOCTYPE html>
<html>
<head>
  <title>User CRUD</title>
</head>
<body>
  <h1>User List</h1>
  <a href="/create">Add New User</a>
  <ul>
    <% users.forEach(user => { %>
      <li>
        <%= user.name %> - <%= user.email %> - <%= user.age %>
        <a href="/edit/<%= user._id %>">Edit</a>
        <a href="/delete/<%= user._id %>">Delete</a>
      </li>
    <% }) %>
  </ul>
</body>
</html>
